{
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "extends": ["airbnb", "react-app", "prettier"],
    "rules": {
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-one-expression-per-line": "off",
    "no-param-reassign": "warn",
    "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
    ]
    },
    "env": {
    "es6": true
    }
}